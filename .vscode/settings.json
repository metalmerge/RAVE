    "anuary",
    "biasy",
    "blackbaud",
    "ctober",
    "ebruary",
    "ecember",
    "eptember",
    "onth",
    "ovember",
    "pril",
    "pyautogui",
    "spacy",
    "tesseract",
    "ugust"
  ]
}
